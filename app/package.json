{
  "name": "ai-code-terminal",
  "version": "2.0.0",
  "description": "Single-tenant terminal interface with god-mode shell access (Docker deployment)",
  "license": "AGPL-3.0-or-later",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "test:controllers": "jest __tests__/controllers/",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:passing": "jest __tests__/controllers/",
    "docker:build": "docker build -t claude-code-web .",
    "docker:run": "docker run -p 3014:3014 --env-file .env claude-code-web"
  },
  "dependencies": {
    "@octokit/rest": "^22.0.0",
    "@prisma/client": "^6.13.0",
    "cors": "^2.8.5",
    "ejs": "^3.1.10",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "node-pty": "^1.0.0",
    "prisma": "^6.13.0",
    "socket.io": "^4.7.4",
    "sqlite3": "^5.1.7",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@types/node": "^24.2.1",
    "c8": "^10.1.3",
    "jest": "^30.0.5",
    "jsdom": "^26.1.0",
    "nodemon": "^3.0.2",
    "supertest": "^7.1.4"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
